{
    "name": "databasemodule/databasemodule",
    "description": "A module to work with databases in PHP without rewrite of code.",
    "type": "project",
    "license": "MIT",
    "readme": "./README.md",
    "prefer-stable": true,
    "minimum-stability": "stable",
    "keywords": [ "sql", "modules" ],
    "authors": [
        {
            "name": "Cassiano Castro",
            "email": "development.castro@gmail.com",
            "role": "Lead"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/app/"
        }
    },
	"autoload-dev": {
        "psr-4": {
            "Tests\\": "src/tests/"
        }
    },
	"config": {
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": false
	},
    "require": {
		"php": ">=8.1.2"
	}
}